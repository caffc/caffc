module main

native {
#include <stdio.h>
}

print_value(i32 value) {
  native {
    printf("value: %d\n", value);
  }
}

main() {
  u8[][] arr = new u8[2][2]

  arr[0][0] = 0
  arr[0][1] = 1
  arr[1][0] = 2
  arr[1][1] = 3

  print_value(arr[0][1]) // expect 1
  arr[0][1] = arr[1][0]
  print_value(arr[0][1]) // expect 2
}

