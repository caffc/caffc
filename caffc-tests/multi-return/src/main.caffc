module main

greeting(str name) -> str header, str footer {
  header = "hello "
  header = header.add(name)
  footer = "have a good day "
  footer = footer.add(name)

  return header, footer
}

hello(str name) -> str header, str footer {
  str header = "hello "
  str footer = "have a good day "

  return header.add(name), footer.add(name)
}

main() {
  str h, f

  h, f = greeting("world")

  print(h)
  print(f)

  h, f = hello("world")

  print(h)
  print(f)
}
